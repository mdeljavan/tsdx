{
  "name": "utsdx",
  "version": "0.14.1",
  "author": "Jared Palmer <jared@palmer.net>",
  "description": "Zero-config TypeScript package development",
  "license": "MIT",
  "homepage": "https://github.com/mdeljavan/tsdx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdeljavan/tsdx.git"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup"
  ],
  "bugs": {
    "url": "https://github.com/mdeljavan/tsdx/issues"
  },
  "bin": {
    "utsdx": "./dist/index.js"
  },
  "scripts": {
    "prepare": "tsc -p tsconfig.json && husky install",
    "build": "tsc -p tsconfig.json",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/index.js lint ./ --ignore-pattern 'test/e2e/fixtures/lint'",
    "test": "yarn build && yarn test:post-build",
    "test:post-build": "node dist/index.js test",
    "start": "tsc -p tsconfig.json --watch",
    "release": "np",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "deduplicate:check": "yarn-deduplicate -s fewer yarn.lock --list --fail"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/helper-module-imports": "^7.16.0",
    "@babel/parser": "^7.16.2",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-syntax-flow": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/traverse": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "ansi-escapes": "^4.2.1",
    "asyncro": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-polyfill-regenerator": "^0.2.3",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "jpjs": "^1.2.1",
    "lodash.merge": "^4.6.2",
    "ora": "^4.0.3",
    "pascal-case": "^3.1.2",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "progress-estimator": "^0.3.0",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.59.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sade": "^1.7.4",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "tiny-glob": "^0.2.9",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/rollup-plugin-json": "^3.0.3",
    "@types/sade": "^1.7.3",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.9",
    "@types/styled-components": "^5.1.15",
    "autoprefixer": "^10.4.0",
    "babel-plugin-replace-identifiers": "^0.1.1",
    "cssnano": "^5.0.8",
    "doctoc": "^2.1.0",
    "husky": "^7.0.0",
    "np": "^7.5.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "styled-components": "^5.3.3",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '!test/tests/lint/**' && yarn lint && yarn deduplicate:check && doctoc README.md"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  }
}
